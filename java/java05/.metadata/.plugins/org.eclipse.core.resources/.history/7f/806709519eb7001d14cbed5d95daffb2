		/*int// tipo primitive*/ 
		long nota = 4;
		if (nota>5) {
			System.out.println("Nota aprobada " + nota +".");
		}
		System.out.println("Continua con el control de flujo.");
		
		if (nota>=5) {
			System.out.println("Nota aprobada: " + nota + ".");
			}
			else {
				System.out.println("Nota no aprobada: " + nota + ".");
		}
		
